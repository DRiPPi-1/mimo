#header{
      color: green;
      background-color: pink;
    }

#review-button {
    background-color: rgb(50, 0, 200);
    border-color: none;
    border-style: solid;
    border-width: 1px;
    color: white;
    height: 36px;
    width: 100px;
    border-radius: 2px;
    cursor: pointer;
    transition: background-color 0.15s,
      color 0.15s;
  }
  
#review-button:hover {
    background-color: white;
    color: rgb(41, 118, 211);
  }
  
#review-button:active {
    opacity: 0.7;
  }
    
#reviewText{
  background-color: pink;
  resize: none;
 }
  
#changeBodyColor {
  background-color: rgb(70, 70, 70);
}

h3{
  color: pink
